{
  "name": "loremize",
  "version": "1.0.0",
  "description": "Simple JavaScript-based sample text generator.",
  "main": "./dist/loremize.js",
  "scripts": {
    "lint": "eslint ./src ./scripts --ext .js",
    "build:clean": "babel-node --presets es2015 scripts/cleanup-build-dir.js",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "prepublish": "npm run build:clean && npm run build:dev && npm run build:prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kaishiyoku/loremize.git"
  },
  "keywords": [
    "lorem",
    "ipsum",
    "text",
    "helper"
  ],
  "author": {
    "name": "Andreas Wiedel",
    "email": "ich@andreas-wiedel.de",
    "url": "https://www.andreas-wiedel.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kaishiyoku/loremize/issues"
  },
  "homepage": "https://github.com/Kaishiyoku/loremize#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "del": "^3.0.0",
    "eslint": "^4.17.0",
    "eslint-plugin-fp": "^2.3.0",
    "lodash": "^4.17.5",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0"
  }
}
