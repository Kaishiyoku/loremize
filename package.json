{
  "name": "loremize",
  "version": "1.0.0",
  "description": "Simple JavaScript-based sample text generator.",
  "main": "./dist/loremize.js",
  "scripts": {
    "lint": "eslint ./src ./scripts --ext .js",
    "build:clean": "babel-node --presets es2015 scripts/cleanup-build-dir.js",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "prepublish": "npm run build:clean && npm run build:dev && npm run build:prod",
    "test": "jest --no-cache",
    "test:coverage": "jest --no-cache --coverage --collectCoverageFrom=src/**/*.js",
    "test:watch": "jest --no-cache --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kaishiyoku/loremize.git"
  },
  "keywords": [
    "lorem",
    "ipsum",
    "text",
    "helper"
  ],
  "author": {
    "name": "Andreas Wiedel",
    "email": "ich@andreas-wiedel.de",
    "url": "https://www.andreas-wiedel.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kaishiyoku/loremize/issues"
  },
  "homepage": "https://github.com/Kaishiyoku/loremize#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.0.1",
    "eslint": "^4.17.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-no-unsafe-chars": "^1.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.4.3",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "del": "^3.0.0",
    "lodash": "^4.17.5",
    "ramda": "^0.25.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!ramda)"
    ],
    "testRegex": "/__tests__/.*\\.(js)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {

    },
    "globals": {
      "DEVELOPMENT": false,
      "FAKE_SERVER": false
    },
    "setupFiles": [

    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
