{
  "name": "loremize",
  "version": "1.2.0",
  "description": "Simple JavaScript-based sample text generator.",
  "main": "dist/loremizer.cjs.js",
  "module": "dist/loremizer.esm.js",
  "browser": "dist/loremizer.umd.js",
  "scripts": {
    "lint": "eslint ./src ./scripts --ext .js",
    "build:clean": "babel-node --presets es2015 scripts/cleanup-build-dir.js",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublish": "npm run build:clean && npm run build",
    "pretest": "npm run build",
    "test": "jest --no-cache",
    "test:coverage": "jest --no-cache --coverage --collectCoverageFrom=src/**/*.js",
    "test:watch": "jest --no-cache --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kaishiyoku/loremize.git"
  },
  "keywords": [
    "lorem",
    "ipsum",
    "text",
    "helper"
  ],
  "author": {
    "name": "Andreas Wiedel",
    "email": "ich@andreas-wiedel.de",
    "url": "https://www.andreas-wiedel.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kaishiyoku/loremize/issues"
  },
  "homepage": "https://github.com/Kaishiyoku/loremize#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "23.0.1",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-imports": "1.5.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-minify": "0.4.3",
    "babel-preset-stage-2": "6.24.1",
    "del": "3.0.0",
    "eslint": "4.19.1",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-no-unsafe-chars": "1.0.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.1.0",
    "rollup": "0.60.7",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-terser": "1.0.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.10",
    "ramda": "^0.25.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!ramda)"
    ],
    "testRegex": "/__tests__/.*\\.(js)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {},
    "globals": {
      "DEVELOPMENT": false,
      "FAKE_SERVER": false
    },
    "setupFiles": [],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
